{
    "dbname": "shipsta",
    "description": "船舶静态数据，包括shipid/imono/shipname/mmsi之间的绑定,imono是唯一的，mmsi是可以更换的，shipid是爬取网站的自有唯一标识，不作为本数据库的业务",
    "table": [
        {
            "key": "shipid",
            "type": "text"
        },
        {
            "key": "imono",
            "type": "text"
        },
        {
            "key": "mmsi",
            "type": "text"
        },
        {
            "key": "shipname",
            "type": "text"
        },
        {
            "key": "callsign",
            "type": "text"
        },
        {
            "key": "shiptype",
            "type": "text"
        },
        {
            "key": "shipsubtype",
            "type": "text"
        },
        {
            "key": "length",
            "type": "number"
        },
        {
            "key": "width",
            "type": "number"
        },
        {
            "key": "createtime",
            "type": "timestamp"
        }
    ]
}