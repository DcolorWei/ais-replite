{
    "dbname": "shipdyn",
    "description": "船舶动态数据，包括位置坐标、速度、航向、时间戳等",
    "table": [
        {
            "key": "shipid",
            "type": "text"
        },
        {
            "key": "lon",
            "type": "decimal"
        },
        {
            "key": "lat",
            "type": "decimal"
        },
        {
            "key": "speed",
            "type": "decimal"
        },
        {
            "key": "course",
            "type": "decimal"
        },
        {
            "key": "heading",
            "type": "decimal"
        },
        {
            "key": "rot",
            "type": "decimal"
        },
        {
            "key": "draught",
            "type": "decimal"
        },
        {
            "key": "destination",
            "type": "text"
        },
        {
            "key": "eta",
            "type": "timestamp"
        },
        {
            "key": "status",
            "type": "text"
        },
        {
            "key": "station",
            "type": "text"
        },
        {
            "key": "timestamp",
            "type": "timestamp"
        }
    ]
}